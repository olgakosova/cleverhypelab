<script>
    document.addEventListener("DOMContentLoaded", () => {


        var $element = $('.counters');
            let counter_ = 0;
            start_counter = false;
            $(window).scroll(function() {
                var scroll = $(window).scrollTop() + $(window).height();
            //Если скролл до конца елемента
            var offset = $element.offset().top + $element.height();
            //Если скролл до начала елемента
            //var offset = $element.offset().top
        
            if (scroll > offset && counter_ == 0) {
                counter_ = 1;
                $('.count_item .count').each(function () {
                var $this = $(this);
                jQuery({ Counter: 0 }).animate({ Counter: $this.text() }, {
                    duration: 1700,
                    easing: 'swing',
                    step: function () {
                        $this.text(Math.ceil(this.Counter));
                    }
                });
            });
            }
        });

        $('input[name="search"]').on("input",function(){
            
          1<=$(this).val().length?$(".input-block-dropdown").addClass("active"):$(".input-block-dropdown").removeClass("active")
        }).bind('keydown', function(e) {
            if (e.keyCode == 13) {
                ttolltip();
                return false;
            }
        });
        $('.tooltip').on('click',function(){
            ttolltip();
        });
        function ttolltip(){
            if($('a.input-block-dropdown-el').length){
                showtolltip();
                $('.tooltip span').html('Оберіть уповноваженого з випадаючого списку');
            } else {
                $('.tooltip span').html('Введіть пошуковий запит');
                showtolltip();
            }
        }
        
        function showtolltip(){
            $('.tooltip').addClass('active');
            setTimeout(function(){
                hidetolltip();
            }, 3000);
        }
        function hidetolltip(){
            $('.tooltip').removeClass('active');
        }
        $(document).on('input', '[name="search"]', function(e) {
            
            if($(this).val().length>2){
              node = this;
              data = $('.s_block_right').serialize();

              $.ajax({
                  url: '/efektyvnist-upovnovazhenyh/getuser/',
                  type: 'post',
                  data: data,
                  dataType: 'json',
                  beforeSend: function(){},
                  complete: function(){},
                  success: function(json) {
                      console.log(json);
                    if(json['html']){
                      $('#doc_result').html(json['html']);
                    }
                  },
                  error: function(xhr, ajaxOptions, thrownError) {
                      console.log(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                  }
              });

            } else {

              $('#doc_result').html('');

            }
        })
      });
</script>
<script>
(function () {
function a(a, b, c, d) {
    function e(a, b) {
        a = a || 0, !b || (p.innerHTML = '');
        var c = v[a];
        v.forEach(function (a) {
            a.distance = i(c, a)
        });
        var e = v.map(function (b, c) {
            if (c !== a) return b
        });
        e.sort(function (c, a) {
            return c.distance > a.distance ? 1 : -1
        }), e = e.filter(function (a, b) {
            if (b < r) return a.angle = l(a, c), a
        }), e.sort(function (c, a) {
            return c.angle > a.angle ? 1 : -1
        });
        for (var f = ' ', g = 0; g < e.length; g++) f += 0 === g ? 'M ' : 1 === g ? 'L ' : ' ', f += e[g].X + ' ' + e[g].Y + ' ';
        f += ' L ' + e[0].X + ' ' + e[0].Y + ' ';
        var h = f + ' Z',
            j = document.createElementNS(u, 'path');
        j.setAttribute('d', h), j.setAttribute('stroke', 'rgba(' + y + ',' + z + ',' + A + ',.01)'), 
        j.setAttribute('stroke-width', '0px'), j.setAttribute('fill', 'rgba(' + y + ',' + z + ',' + A + ',.04)'), p.appendChild(j)
    }

    function f() {
        n.setAttribute('width', w), n.setAttribute('height', x), t.setAttribute('width', w), t.setAttribute('height', x), n.setAttribute('viewbox', '0 0 ' + w + ' ' + x)
    }

    function g() {
        v = [];
        for (var a = 0; a < q; a++) v.push(new k(parseInt(Math.random() * (w - 20) + 10), parseInt(Math.random() * (x - 20) + 10), a));
        o.innerHTML = '', p.innerHTML = '', h(), j()
    }

    function h() {
        p.innerHTML = '';
        for (var a = 0; a < v.length; a++) e(a)
    }

    function j() {
        for (var a = 0; a < v.length; a++) v[a].move(s);
        h(), requestAnimationFrame(j)
    }

    function k(a, b, c) {
        return {
            move: function b(a) {
                this.X += this.dx * a, this.Y += this.dy * a, (0 > this.X || this.X > w) && (this.X -= this.dx * a, this.dx = -this.dx), (0 > this.Y || this.Y > x) && (this.Y -= this.dy * a, this.dy = -this.dy)
            },
            X: a,
            Y: b,
            distance: -1,
            id: c,
            angle: -1,
            dx: Math.random() - Math.random(),
            dy: Math.random() - Math.random()
        }
    }

    function i(a, b) {
        var c = 0,
            d = 0;
        return c = b.X - a.X, c *= c, d = b.Y - a.Y, d *= d, Math.sqrt(c + d)
    }

    function l(a, b) {
        var c = a.Y - b.Y,
            d = a.X - b.X,
            e = Math.atan2(c, d),
            f = 180 * e / Math.PI % 360;
        return f = 0 > f ? 360 + f : f, f
    }
    var m = a,
        n = a.querySelector('svg'),
        o = n.querySelector('#points'),
        p = n.querySelector('#polygons'),
        q = b,
        r = c,
        s = .7,
        t = n.querySelector('#bg'),
        u = n.namespaceURI,
        v = [],
        w = m.clientWidth,
        x = m.clientHeight,
        y = d[0] ? d[0] : 255,
        z = d[1] ? d[1] : 255,
        A = d[2] ? d[2] : 255;
    f(), window.onresize = f, g()
}
var b = document.querySelectorAll('.polygon');
'undefined' != typeof b && null != b && b.forEach(function (b) {
    var c = b.dataset.point,
        d = b.dataset.edges,
        e = void 0;
    e = b.dataset.color ? b.dataset.color.split(',') : [], b.innerHTML = '<svg fill="transparent"><rect id="bg"/><g id="main"><g id="polygons"></g><g id="points"></g></g></svg>', new a(b, c, d, e)
})
})();
</script>

